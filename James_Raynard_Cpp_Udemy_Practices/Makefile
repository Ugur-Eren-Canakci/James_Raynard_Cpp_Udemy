.PHONY: clean All

All:
	@echo "----------Building project:[ Virtual_Functions - Debug ]----------"
	@cd "Virtual_Functions" && "$(MAKE)" -f  "Virtual_Functions.mk"
clean:
	@echo "----------Cleaning project:[ Virtual_Functions - Debug ]----------"
	@cd "Virtual_Functions" && "$(MAKE)" -f  "Virtual_Functions.mk" clean
