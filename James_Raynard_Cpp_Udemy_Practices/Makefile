.PHONY: clean All

All:
	@echo "----------Building project:[ Perfect_Forwarding_Practical - Debug ]----------"
	@cd "Perfect_Forwarding_Practical" && "$(MAKE)" -f  "Perfect_Forwarding_Practical.mk"
clean:
	@echo "----------Cleaning project:[ Perfect_Forwarding_Practical - Debug ]----------"
	@cd "Perfect_Forwarding_Practical" && "$(MAKE)" -f  "Perfect_Forwarding_Practical.mk" clean
