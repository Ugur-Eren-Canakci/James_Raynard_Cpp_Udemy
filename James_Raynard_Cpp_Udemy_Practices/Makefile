.PHONY: clean All

All:
	@echo "----------Building project:[ Interfacing_to_C - Debug ]----------"
	@cd "Interfacing_to_C" && "$(MAKE)" -f  "Interfacing_to_C.mk"
clean:
	@echo "----------Cleaning project:[ Interfacing_to_C - Debug ]----------"
	@cd "Interfacing_to_C" && "$(MAKE)" -f  "Interfacing_to_C.mk" clean
