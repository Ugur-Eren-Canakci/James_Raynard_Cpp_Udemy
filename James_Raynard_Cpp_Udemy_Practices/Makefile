.PHONY: clean All

All:
	@echo "----------Building project:[ Static_and_Dynamic_Type - Debug ]----------"
	@cd "Static_and_Dynamic_Type" && "$(MAKE)" -f  "Static_and_Dynamic_Type.mk"
clean:
	@echo "----------Cleaning project:[ Static_and_Dynamic_Type - Debug ]----------"
	@cd "Static_and_Dynamic_Type" && "$(MAKE)" -f  "Static_and_Dynamic_Type.mk" clean
