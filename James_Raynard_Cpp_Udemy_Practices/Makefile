.PHONY: clean All

All:
	@echo "----------Building project:[ Base_and_Derived_Classes - Debug ]----------"
	@cd "Base_and_Derived_Classes" && "$(MAKE)" -f  "Base_and_Derived_Classes.mk"
clean:
	@echo "----------Cleaning project:[ Base_and_Derived_Classes - Debug ]----------"
	@cd "Base_and_Derived_Classes" && "$(MAKE)" -f  "Base_and_Derived_Classes.mk" clean
