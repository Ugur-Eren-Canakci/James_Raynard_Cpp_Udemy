.PHONY: clean All

All:
	@echo "----------Building project:[ Exceptions_and_Special_Member_Functions - Debug ]----------"
	@cd "Exceptions_and_Special_Member_Functions" && "$(MAKE)" -f  "Exceptions_and_Special_Member_Functions.mk"
clean:
	@echo "----------Cleaning project:[ Exceptions_and_Special_Member_Functions - Debug ]----------"
	@cd "Exceptions_and_Special_Member_Functions" && "$(MAKE)" -f  "Exceptions_and_Special_Member_Functions.mk" clean
