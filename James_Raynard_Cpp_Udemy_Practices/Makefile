.PHONY: clean All

All:
	@echo "----------Building project:[ decltype_keyword - Debug ]----------"
	@cd "decltype_keyword" && "$(MAKE)" -f  "decltype_keyword.mk"
clean:
	@echo "----------Cleaning project:[ decltype_keyword - Debug ]----------"
	@cd "decltype_keyword" && "$(MAKE)" -f  "decltype_keyword.mk" clean
