.PHONY: clean All

All:
	@echo "----------Building project:[ Member_Function_Pointers - Debug ]----------"
	@cd "Member_Function_Pointers" && "$(MAKE)" -f  "Member_Function_Pointers.mk"
clean:
	@echo "----------Cleaning project:[ Member_Function_Pointers - Debug ]----------"
	@cd "Member_Function_Pointers" && "$(MAKE)" -f  "Member_Function_Pointers.mk" clean
