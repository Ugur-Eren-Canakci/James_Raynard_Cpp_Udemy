.PHONY: clean All

All:
	@echo "----------Building project:[ Unique_Pointers_and_Custom_Deleters - Debug ]----------"
	@cd "Unique_Pointers_and_Custom_Deleters" && "$(MAKE)" -f  "Unique_Pointers_and_Custom_Deleters.mk"
clean:
	@echo "----------Cleaning project:[ Unique_Pointers_and_Custom_Deleters - Debug ]----------"
	@cd "Unique_Pointers_and_Custom_Deleters" && "$(MAKE)" -f  "Unique_Pointers_and_Custom_Deleters.mk" clean
