.PHONY: clean All

All:
	@echo "----------Building project:[ Unique_Pointers_and_Polymorphism - Debug ]----------"
	@cd "Unique_Pointers_and_Polymorphism" && "$(MAKE)" -f  "Unique_Pointers_and_Polymorphism.mk"
clean:
	@echo "----------Cleaning project:[ Unique_Pointers_and_Polymorphism - Debug ]----------"
	@cd "Unique_Pointers_and_Polymorphism" && "$(MAKE)" -f  "Unique_Pointers_and_Polymorphism.mk" clean
