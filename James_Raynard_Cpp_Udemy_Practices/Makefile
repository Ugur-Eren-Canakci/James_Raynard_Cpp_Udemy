.PHONY: clean All

All:
	@echo "----------Building project:[ Shared_Pointers - Debug ]----------"
	@cd "Shared_Pointers" && "$(MAKE)" -f  "Shared_Pointers.mk"
clean:
	@echo "----------Cleaning project:[ Shared_Pointers - Debug ]----------"
	@cd "Shared_Pointers" && "$(MAKE)" -f  "Shared_Pointers.mk" clean
