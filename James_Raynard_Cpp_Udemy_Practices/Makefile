.PHONY: clean All

All:
	@echo "----------Building project:[ Unions_Continued - Debug ]----------"
	@cd "Unions_Continued" && "$(MAKE)" -f  "Unions_Continued.mk"
clean:
	@echo "----------Cleaning project:[ Unions_Continued - Debug ]----------"
	@cd "Unions_Continued" && "$(MAKE)" -f  "Unions_Continued.mk" clean
