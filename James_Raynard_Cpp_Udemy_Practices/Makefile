.PHONY: clean All

All:
	@echo "----------Building project:[ Unique_Pointer - Debug ]----------"
	@cd "Unique_Pointer" && "$(MAKE)" -f  "Unique_Pointer.mk"
clean:
	@echo "----------Cleaning project:[ Unique_Pointer - Debug ]----------"
	@cd "Unique_Pointer" && "$(MAKE)" -f  "Unique_Pointer.mk" clean
