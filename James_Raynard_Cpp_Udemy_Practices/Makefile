.PHONY: clean All

All:
	@echo "----------Building project:[ Perfect_Forwarding - Debug ]----------"
	@cd "Perfect_Forwarding" && "$(MAKE)" -f  "Perfect_Forwarding.mk"
clean:
	@echo "----------Cleaning project:[ Perfect_Forwarding - Debug ]----------"
	@cd "Perfect_Forwarding" && "$(MAKE)" -f  "Perfect_Forwarding.mk" clean
