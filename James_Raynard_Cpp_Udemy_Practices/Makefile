.PHONY: clean All

All:
	@echo "----------Building project:[ Reference_Counting - Debug ]----------"
	@cd "Reference_Counting" && "$(MAKE)" -f  "Reference_Counting.mk"
clean:
	@echo "----------Cleaning project:[ Reference_Counting - Debug ]----------"
	@cd "Reference_Counting" && "$(MAKE)" -f  "Reference_Counting.mk" clean
