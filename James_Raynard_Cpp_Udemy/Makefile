.PHONY: clean All

All:
	@echo "----------Building project:[ Smart_Pointers - Debug ]----------"
	@cd "Smart_Pointers" && "$(MAKE)" -f  "Smart_Pointers.mk"
clean:
	@echo "----------Cleaning project:[ Smart_Pointers - Debug ]----------"
	@cd "Smart_Pointers" && "$(MAKE)" -f  "Smart_Pointers.mk" clean
