.PHONY: clean All

All:
	@echo "----------Building project:[ Move_operators - Debug ]----------"
	@cd "Move_operators" && "$(MAKE)" -f  "Move_operators.mk"
clean:
	@echo "----------Cleaning project:[ Move_operators - Debug ]----------"
	@cd "Move_operators" && "$(MAKE)" -f  "Move_operators.mk" clean
