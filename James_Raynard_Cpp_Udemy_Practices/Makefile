.PHONY: clean All

All:
	@echo "----------Building project:[ Custom_Exception_Class - Debug ]----------"
	@cd "Custom_Exception_Class" && "$(MAKE)" -f  "Custom_Exception_Class.mk"
clean:
	@echo "----------Cleaning project:[ Custom_Exception_Class - Debug ]----------"
	@cd "Custom_Exception_Class" && "$(MAKE)" -f  "Custom_Exception_Class.mk" clean
