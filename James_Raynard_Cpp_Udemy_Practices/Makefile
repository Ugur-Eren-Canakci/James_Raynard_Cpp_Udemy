.PHONY: clean All

All:
	@echo "----------Building project:[ Virtual_Functions_C++11 - Debug ]----------"
	@cd "Virtual_Functions_C++11" && "$(MAKE)" -f  "Virtual_Functions_C++11.mk"
clean:
	@echo "----------Cleaning project:[ Virtual_Functions_C++11 - Debug ]----------"
	@cd "Virtual_Functions_C++11" && "$(MAKE)" -f  "Virtual_Functions_C++11.mk" clean
