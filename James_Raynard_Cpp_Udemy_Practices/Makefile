.PHONY: clean All

All:
	@echo "----------Building project:[ Variadic_Templates - Debug ]----------"
	@cd "Variadic_Templates" && "$(MAKE)" -f  "Variadic_Templates.mk"
clean:
	@echo "----------Cleaning project:[ Variadic_Templates - Debug ]----------"
	@cd "Variadic_Templates" && "$(MAKE)" -f  "Variadic_Templates.mk" clean
