.PHONY: clean All

All:
	@echo "----------Building project:[ Chrono_Clocks_and_Time_Points - Debug ]----------"
	@cd "Chrono_Clocks_and_Time_Points" && "$(MAKE)" -f  "Chrono_Clocks_and_Time_Points.mk"
clean:
	@echo "----------Cleaning project:[ Chrono_Clocks_and_Time_Points - Debug ]----------"
	@cd "Chrono_Clocks_and_Time_Points" && "$(MAKE)" -f  "Chrono_Clocks_and_Time_Points.mk" clean
