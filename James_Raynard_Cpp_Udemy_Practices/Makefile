.PHONY: clean All

All:
	@echo "----------Building project:[ Containers_Workshop - Debug ]----------"
	@cd "Containers_Workshop" && "$(MAKE)" -f  "Containers_Workshop.mk"
clean:
	@echo "----------Cleaning project:[ Containers_Workshop - Debug ]----------"
	@cd "Containers_Workshop" && "$(MAKE)" -f  "Containers_Workshop.mk" clean
