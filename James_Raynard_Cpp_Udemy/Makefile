.PHONY: clean All

All:
	@echo "----------Building project:[ Error_Handling - Debug ]----------"
	@cd "Error_Handling" && "$(MAKE)" -f  "Error_Handling.mk"
clean:
	@echo "----------Cleaning project:[ Error_Handling - Debug ]----------"
	@cd "Error_Handling" && "$(MAKE)" -f  "Error_Handling.mk" clean
