.PHONY: clean All

All:
	@echo "----------Building project:[ Default_and_Delete - Debug ]----------"
	@cd "Default_and_Delete" && "$(MAKE)" -f  "Default_and_Delete.mk"
clean:
	@echo "----------Cleaning project:[ Default_and_Delete - Debug ]----------"
	@cd "Default_and_Delete" && "$(MAKE)" -f  "Default_and_Delete.mk" clean
