.PHONY: clean All

All:
	@echo "----------Building project:[ Constant_Expressions - Debug ]----------"
	@cd "Constant_Expressions" && "$(MAKE)" -f  "Constant_Expressions.mk"
clean:
	@echo "----------Cleaning project:[ Constant_Expressions - Debug ]----------"
	@cd "Constant_Expressions" && "$(MAKE)" -f  "Constant_Expressions.mk" clean
